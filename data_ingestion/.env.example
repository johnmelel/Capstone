# Google Cloud Storage Configuration
GCS_BUCKET_NAME=your-gcs-bucket-name
GCS_BUCKET_PREFIX=optional/folder/path
GOOGLE_SERVICE_ACCOUNT_JSON=service_account.json
GCS_RECURSIVE=true

# Milvus Vector Database Configuration
MILVUS_URI=https://your-milvus-instance.cloud.zilliz.com
MILVUS_API_KEY=your-milvus-api-key
MILVUS_COLLECTION_NAME=pdf_embeddings
MILVUS_METRIC_TYPE=COSINE

# Gemini Embedding Configuration
# API key for Google Gemini embedding models
GEMINI_API_KEY=your-gemini-api-key

# Embedding Model Settings
# Supported models: text-embedding-004, text-embedding-005, gemini-embedding-001
EMBEDDING_MODEL=gemini-embedding-001
# Embedding dimension
EMBEDDING_DIMENSION=3072
# Maximum tokens per chunk
MAX_TOKENS_PER_CHUNK=2048

# Text Chunking Configuration
# Target tokens per chunk (default 1792 leaves 256 token buffer)
CHUNK_SIZE=1792
# Tokens to overlap between chunks (default 64 tokens â‰ˆ 16 words)
CHUNK_OVERLAP=64
# Batch size for processing embeddings
BATCH_SIZE=100

# MinerU PDF Extraction Configuration
# Backend: pipeline, vlm-transformers, vlm-vllm-engine
MINERU_BACKEND=pipeline
# Model source: huggingface, modelscope, local
MINERU_MODEL_SOURCE=huggingface
# Language: en, ch, auto, etc.
MINERU_LANG=en
# Timeout for PDF extraction in seconds (default 15 minutes)
PDF_EXTRACTION_TIMEOUT=900
# Debug mode for MinerU
MINERU_DEBUG_MODE=false
# Enable table extraction
MINERU_ENABLE_TABLES=false
# Enable formula extraction
MINERU_ENABLE_FORMULAS=false

# Retrieval Test Configuration
DEFAULT_RETRIEVAL_PROMPT=What is the future of AI?
TOP_K_RESULTS=5
